
@using System.Configuration
@using System.Web
@using MvcReportViewer;

@{
    ViewBag.Title = "MonthlyReport";
    Layout = null;
}

@{
    var parameters = new List<KeyValuePair<string, object>>
{
        new KeyValuePair<string, object>("VendorId",ViewBag.vendorId),
        new KeyValuePair<string, object>("issueMonth",ViewBag.month),
        new KeyValuePair<string, object>("issueYear",ViewBag.issueYear),
        new KeyValuePair<string, object>("VehicleType",ViewBag.vehicleType),
        new KeyValuePair<string, object>("isShowTotal",ViewBag.isShowTotal),
        new KeyValuePair<string, object>("iswithEndDate",ViewBag.iswithEndDate),
       

    };
    var settings = new ControlSettings { ShowToolBar = false };
}
<div id="divPrint">

    @Html.MvcReportViewer("/RMReports/Monthly_Report", parameters, new { Height = 800, Width = "100%", style = "border: none" }).ControlSettings(new ControlSettings { ShowParameterPrompts = false })
    <div class="span3"></div>
</div>



