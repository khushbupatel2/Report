@using System.Configuration
@using System.Web
@using MvcReportViewer;

@{
    ViewBag.Title = "OutSide Invoice Details";
    Layout = null;

}

@{
    var parameters = new List<KeyValuePair<string, object>>
{
    new KeyValuePair<string, object>("vendorid",ViewBag.vendorid),
    new KeyValuePair<string, object>("unitId",ViewBag.unitId),
    new KeyValuePair<string, object>("division",ViewBag.division),
    new KeyValuePair<string, object>("unitType",ViewBag.unitType),
    new KeyValuePair<string, object>("locationId",ViewBag.locationId),
    new KeyValuePair<string, object>("conversionRate",ViewBag.conversionRate),
    new KeyValuePair<string, object>("fromDate",ViewBag.fromDate),
    new KeyValuePair<string, object>("searchWithoutTime",ViewBag.searchWithoutTime),
new KeyValuePair<string, object>("totime",ViewBag.totime),
    };
    var settings = new ControlSettings { ShowToolBar = false };
}
<div id="divPrint">

    @Html.MvcReportViewer("/RMReports/OutsideInvoiceDetails", parameters, new { Height = 1000, Width = "100%", style = "border: none" }).ControlSettings(new ControlSettings { ShowParameterPrompts = false })
    <div class="span3"></div>

</div>

