@using System.Configuration
@using System.Web
@using MvcReportViewer;

@{
    ViewBag.Title = "Truck Inspection";
    Layout = null;

}

@{
    var parameters = new List<KeyValuePair<string, object>>
{
    new KeyValuePair<string, object>("id",ViewBag.id)

    };
    var settings = new ControlSettings { ShowToolBar = false };
}
<div id="divPrint">

    @Html.MvcReportViewer("/RMReports/TruckUSAInspectionForm", parameters, new { Height = 1000, Width = "100%", style = "border: none" }).ControlSettings(new ControlSettings { ShowParameterPrompts = false })
    <div class="span3"></div>
</div>



