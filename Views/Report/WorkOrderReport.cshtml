
@using System.Configuration
@using System.Web
@using MvcReportViewer;

@{
    ViewBag.Title = "WorkOrder";
    Layout = null;
}

@{
    var parameters = new List<KeyValuePair<string, object>>
    {
    new KeyValuePair<string, object>("id",ViewBag.Id),
    
    };
    var settings = new ControlSettings { ShowToolBar = false };
}
<div id="divPrint">

    @Html.MvcReportViewer("/RMReports/WorkOrder", parameters, new { Height = 800, Width = "100%", style = "border: none" }).ControlSettings(new ControlSettings { ShowParameterPrompts = false })
    <div class="span3"></div>
</div>



