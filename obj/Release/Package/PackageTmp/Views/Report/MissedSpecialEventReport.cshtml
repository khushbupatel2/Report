@using System.Configuration
@using System.Web
@using MvcReportViewer;

@{
    ViewBag.Title = "Missed Event Report";
    Layout = null;

}

@{
        var parameters = new List<KeyValuePair<string, object>>
{
    new KeyValuePair<string, object>("startDate",ViewBag.fromDate),
    new KeyValuePair<string, object>("endDate",ViewBag.toDate),
    new KeyValuePair<string, object>("SECode",ViewBag.SeCode),
    new KeyValuePair<string, object>("UnitId",ViewBag.UnitId),
    new KeyValuePair<string, object>("location",ViewBag.Terminal)
 
    };
    var settings = new ControlSettings { ShowToolBar = false };
}
<div id="divPrint">

    @Html.MvcReportViewer("/RMReports/MissedEventReport", parameters, new { Height = 1000, Width = "100%", style = "border: none" }).ControlSettings(new ControlSettings { ShowParameterPrompts = false })
    <div class="span3"></div>
</div>



