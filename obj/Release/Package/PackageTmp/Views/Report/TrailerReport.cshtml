
@using System.Configuration
@using System.Web
@using MvcReportViewer;

@{
    ViewBag.Title = "Trailer Report";
    Layout = null;

}

@{
    var parameters = new List<KeyValuePair<string, object>>
{
    new KeyValuePair<string, object>("unitId",ViewBag.unitId),
    new KeyValuePair<string, object>("FromDate",ViewBag.FromDate),
    new KeyValuePair<string, object>("ToDate",ViewBag.ToDate),
    new KeyValuePair<string, object>("issueCategory",ViewBag.issueCategory),
    new KeyValuePair<string, object>("currencyRate",ViewBag.rate),
     new KeyValuePair<string, object>("isfixwithCategory",ViewBag.isfixwithCategory),
      new KeyValuePair<string, object>("isActive",ViewBag.activeInDispatch)
     


    };
    var settings = new ControlSettings { ShowToolBar = false };
}
<div id="divPrint">

    @Html.MvcReportViewer("/RMReports/TrailerReport", parameters, new { Height = 800, Width = "100%", style = "border: none" }).ControlSettings(new ControlSettings { ShowParameterPrompts = false })
    <div class="span3"></div>
</div>




